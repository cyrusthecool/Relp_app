<h1>Edit Category</h1>



<%= form_for(@category, :html => {:multipart => true}) do |f| %>
<div class="form-group">
  <%= f.label :name%>
  <%= f.text_field :name, class: "form-control"%>
  <br>
  <%# f.label :image %>
  <%# f.cl_image_upload :image, class: "form-control"%>
  <br>
  <%= f.label :detail%>
  <%= f.text_area :detail, class: "form-control" %>
  <br>
  <%= f.submit class:"btn btn-primary"%>
<% end %>
</div>
